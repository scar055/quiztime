<Window x:Class="QuizTime.quizAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizTime"
        mc:Ignorable="d"
        Title="quizAdd" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txbQuizName" HorizontalAlignment="Left" Height="22" Margin="349,197,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="144"/>
        <Button x:Name="addQuiz" Content="voeg toe" HorizontalAlignment="Left" Margin="698,377,0,0" VerticalAlignment="Top" Width="86" Height="33" Click="addQuiz_Click"/>
        <Label Content="Voer hier de quiz naam in:" HorizontalAlignment="Left" Margin="181,193,0,0" VerticalAlignment="Top"/>
        <Button x:Name="cancelQuiz" Content="cancel" HorizontalAlignment="Left" Margin="10,377,0,0" VerticalAlignment="Top" Width="81" Height="33" Click="cancelQuiz_Click"/>
        <Label Content="Quiz" HorizontalAlignment="Left" Margin="390,93,0,0" VerticalAlignment="Top" FontWeight="Bold" Height="37" Width="61" FontSize="22"/>
    </Grid>
</Window>
